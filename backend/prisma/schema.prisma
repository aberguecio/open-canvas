generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

model Image {
  id        Int      @id @default(autoincrement())
  key       String
  name      String
  userEmail String   @default("test@test.com")
  createdAt DateTime @default(now())
}
